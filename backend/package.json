{
  "name": "askyourdb-backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "seed": "ts-node src/scripts/seedDatabase.ts",
    "seed:mongo": "ts-node src/scripts/seedDatabase.ts",
    "seed:postgres": "ts-node src/scripts/seedPostgres.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.8.0",
    "groq-sdk": "^0.37.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.5.1",
    "mongoose": "^7.3.1",
    "openai": "^4.14.0",
    "pg": "^8.11.0",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.7",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  }
}
